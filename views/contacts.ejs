<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <%include partials/import-header.ejs %>
    <title>Contacts</title>
</head>

<body id=<%= user.uid %>>
    <div class="show contacts-panel wrapper animated fadeIn delay-1s">
        <div class="panel">
            <button id='settings'><i class="material-icons">settings</i></button>
            <button id='search'><i class="material-icons">search</i></button>
            <h1 class='page-title'>Contacts</h1>
            <button id='add'><i class="material-icons">add</i></button>

        </div>

        <div class="contacts">
            <% include partials/user-contact %>

        </div>
    </div>

    <div class="hide settings wrapper animated">

        <div class="panel">
            <button id='back-to-contacts'><i class="material-icons">arrow_back</i></button>
            <button id='back-to-contacts'><i class="fas fa-info"></i></button>
            <h1 class='page-title'>Settings</h1>
            <button id='back-to-contacts'><i class="material-icons">delete</i></button>
        </div>

    </div>

    <%include partials/add-contacts.ejs%>
    <%include partials/popup-add-contact.ejs%>



    <div class="hide" id="cover"></div>

</body>
<%include partials/libraries %>

</html>